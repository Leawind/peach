<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>
<BorderPane minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1">
   <bottom>
      <FlowPane alignment="CENTER_RIGHT" hgap="10.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="previous" mnemonicParsing="false" onAction="#onPrevious" text="%ui.wizard.previous"/>
            <Button fx:id="next" mnemonicParsing="false" onAction="#onNext" text="%ui.wizard.next"/>
            <Button mnemonicParsing="false" onAction="#onCancel" text="%ui.wizard.cancel"/>
         </children>
         <BorderPane.margin>
            <Insets top="10.0"/>
         </BorderPane.margin>
      </FlowPane>
   </bottom>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
   </padding>
</BorderPane>
