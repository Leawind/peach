<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<Accordion prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1"
           xmlns:fx="http://javafx.com/fxml/1">
    <panes>
        <TitledPane animated="false" text="%ui.mod_info.general.title">
            <content>
                <AnchorPane>
                    <children>
                        <GridPane hgap="10.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                  AnchorPane.topAnchor="0.0">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0"/>
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            </rowConstraints>
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="%ui.mod_info.display_name"/>
                                <TextField GridPane.columnIndex="1"/>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="%ui.mod_info.id"
                                      GridPane.rowIndex="1"/>
                                <TextField GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                                <TextField GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="%ui.mod_info.version"
                                      GridPane.rowIndex="2"/>
                            </children>
                        </GridPane>
                    </children>
                </AnchorPane>
            </content>
        </TitledPane>
        <TitledPane animated="false" text="%ui.mod_info.advanced.title">
            <content>
                <AnchorPane/>
            </content>
        </TitledPane>
    </panes>
</Accordion>
