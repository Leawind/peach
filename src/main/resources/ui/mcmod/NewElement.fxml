<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<fx:root type="BorderPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <center>
        <GridPane hgap="10.0" BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" minWidth="10.0"
                                   prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="%dialog.newElement.name"/>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="%dialog.newElement.type" GridPane.rowIndex="1"/>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="%dialog.newElement.registry_name"
                      GridPane.rowIndex="2"/>
                <TextField fx:id="name" GridPane.columnIndex="1"/>
                <ChoiceBox fx:id="type" prefWidth="10000.0" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <Text fx:id="registerName" strokeType="OUTSIDE" strokeWidth="0.0" GridPane.columnIndex="1"
                      GridPane.rowIndex="2"/>
            </children>
        </GridPane>
    </center>
    <bottom>
        <FlowPane alignment="CENTER_RIGHT" hgap="10.0" BorderPane.alignment="CENTER">
            <children>
                <Button defaultButton="true" mnemonicParsing="false" onAction="#onFinish" text="%common.finish"/>
                <Button cancelButton="true" mnemonicParsing="false" onAction="#onCancel" text="%common.cancel"/>
            </children>
            <BorderPane.margin>
                <Insets top="10.0"/>
            </BorderPane.margin>
        </FlowPane>
    </bottom>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</fx:root>
