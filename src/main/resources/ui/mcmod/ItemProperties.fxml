<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<FlowPane alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <GridPane hgap="50.0">
            <columnConstraints>
                <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES"/>
                <ColumnConstraints halignment="RIGHT" hgrow="ALWAYS"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="%wizard.itemProperties.registerName"/>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="%wizard.itemProperties.displayName"
                      GridPane.rowIndex="1"/>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="%wizard.itemProperties.itemGroup"
                      GridPane.rowIndex="2"/>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="%wizard.itemProperties.maxStackSize"
                      GridPane.rowIndex="3"/>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="%wizard.itemProperties.effect"
                      GridPane.rowIndex="4"/>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="%wizard.itemProperties.information"
                      GridPane.rowIndex="5"/>
                <TextField fx:id="registerName" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1"/>
                <TextField fx:id="displayName" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1"
                           GridPane.rowIndex="1"/>
                <Spinner fx:id="maxStackSize" editable="true" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1"
                         GridPane.rowIndex="3"/>
                <RadioButton fx:id="effect" mnemonicParsing="false" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
                <TextArea fx:id="information" maxWidth="1.7976931348623157E308" prefHeight="100.0" prefWidth="300.0"
                          GridPane.columnIndex="1" GridPane.rowIndex="5"/>
                <ComboBox fx:id="itemGroup" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1"
                          GridPane.rowIndex="2"/>
            </children>
        </GridPane>
    </children>
</FlowPane>
